{
    "compilerOptions": {
        "lib": ["dom", "es2015", "es2016", "es2017"],
        "jsx": "react",
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": false,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true
    },
    "include": ["public/bundles", "frontend/types.d.ts"]
}
